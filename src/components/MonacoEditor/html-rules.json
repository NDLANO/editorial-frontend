{
  "attributes": {
    "span": {
      "mustContainAtLeastOneOptionalAttribute": true,
      "optional": [["lang"], ["data-size"]]
    },
    "div": {
      "optional": [["class"], ["data-type"]]
    },
    "td": {
      "optional": [["valign"], ["align"], ["colspan"], ["rowspan"]]
    },
    "th": {
      "optional": [["valign"], ["align"], ["colspan"], ["rowspan"]]
    },
    "a": {
      "optional": [["href"], ["title"], ["name"], ["target"], ["rel"]]
    },
    "ol": {
      "optional": [["data-type"]]
    },
    "aside": {
      "optional": [["data-type"]]
    },
    "p": {
      "optional": [["data-align"]]
    },
    "ul": {
      "optional": [["data-type"]]
    }
  },
  "tags": [
    "pre",
    "em",
    "code",
    "h2",
    "h3",
    "h1",
    "h4",
    "table",
    "tbody",
    "section",
    "li",
    "details",
    "tr",
    "sub",
    "sup",
    "thead",
    "blockquote",
    "figcaption",
    "b",
    "br",
    "article",
    "strong",
    "embed",
    "i",
    "u",
    "button",
    "summary",
    "caption",
    "audio",
    "tfoot",
    "td",
    "th"
  ]
}
