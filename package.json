    "eslint-config-ndla": "^0.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flux-standard-action": "^1.2.0",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "nock": "^9.0.9",
