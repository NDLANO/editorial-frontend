{
  "name": "editorial-frontend",
  "version": "0.0.1",
  "description": "System for producing content for NDLA",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js -p --bail",
    "clean": "rimraf ./htdocs/assets/*",
    "lint": "yarn lint-css && yarn lint-prettier && yarn lint-es",
    "lint-css": "stylelint style/**/*.css",
    "lint-es": "eslint --ext .js --ext .jsx src server",
    "lint-prettier": "node prettier.js lint",
    "prebuild": "yarn run clean",
    "pretest": "yarn lint",
    "prettier": "node prettier.js write",
    "start": "cross-env NODE_ENV=development nodemon server --watch server",
    "start-prod": "cross-env NODE_ENV=production node server",
    "test": "cross-env NODE_ENV=unittest jest",
    "tdd": "cross-env NODE_ENV=unittest jest --watch"
  },
  "jest": {
    "testRegex": "/__tests__/.*-test.(js|jsx)$",
    "setupFiles": [
      "./src/__tests__/_initTestEnv.js",
      "./src/__tests__/enzyme-setup.js"
    ]
  },
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
