{
  "name": "editorial-frontend",
  "version": "0.0.1",
  "description": "System for producing content for NDLA",
  "scripts": {
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=2048' ts-node scripts/build",
    "postbuild": "cp build/assets.json build/public/assets.json",
    "clean": "rimraf ./build ./cache",
    "check-all": "yarn lint && yarn tsc --noEmit && yarn test-ci",
    "lint": "yarn lint-css && yarn format-check && yarn lint-es",
    "lint-css": "stylelint src/style/**/*.css",
    "lint-es": "eslint --cache --cache-location '.eslintcache/' --ext .js,.jsx,.ts,.tsx --max-warnings=0 src",
    "format-check": "node prettier.js lint",
    "format": "node prettier.js write",
    "start:tsc": "tsc -b -w --preserveWatchOutput",
    "start": "NODE_OPTIONS='--max-old-space-size=2048' concurrently 'yarn start:tsc' 'ts-node scripts/start-ssr'",
    "start-with-local-converter": "LOCAL_CONVERTER=true yarn start",
    "start-prod": "cross-env NODE_ENV=production node build/server | bunyan",
    "now-start": "cross-env NODE_ENV=production NOW=true node build/server | bunyan",
    "test-ci": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --maxWorkers=4 --ci",
    "test": "cross-env BABEL_ENV=test NODE_ENV=unittest jest",
    "tdd": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --watch",
    "check-articles": "NODE_ENV=test CHECK_ARTICLE_SCRIPT=true node scripts/bin/check-articles",
    "cy": "CYPRESS_USE_FIXTURES=true BABEL_ENV=test cypress open",
    "cy-run": "CYPRESS_USE_FIXTURES=true BABEL_ENV=test cypress run",
    "cy-record": "CYPRESS_RECORD_FIXTURES=true BABEL_ENV=test cypress open",
    "pree2e-ci-record": "cross-env NODE_ENV=production node build/server &",
    "e2e-ci": "cross-env CYPRESS_USE_FIXTURES=true BABEL_ENV=test cypress run --record --parallel",
    "run-e2e": "start-server-and-test 'DISABLE_CSP=true yarn start' :3000 'CYPRESS_USE_FIXTURES=true BABEL_ENV=test cypress run --record --parallel'",
    "run-cyp": "start-server-and-test 'DISABLE_CSP=true yarn start' :3000 'BABEL_ENV=test yarn cy-run'",
    "e2e-use-fixtures": "cross-env CYPRESS_USE_FIXTURES=true BABEL_ENV=test cypress open",
    "e2e-record-fixtures": "cross-env CYPRESS_RECORD_FIXTURES=true BABEL_ENV=test cypress run",
    "e2e-ci-record": "cross-env CYPRESS_USE_FIXTURES=true BABEL_ENV=test cypress run --record",
    "check-translations": "cross-env BABEL_ENV=test NODE_ENV=unittest jest src/phrases/__tests__/"
  },
