{
  "name": "editorial-frontend",
  "version": "0.0.1",
  "description": "System for producing content for NDLA",
  "scripts": {
    "build": "razzle build",
    "postbuild": "cp build/assets.json build/public/assets.json",
    "clean": "rimraf ./build",
    "lint": "yarn lint-css && yarn lint-prettier && yarn lint-es",
    "lint-css": "stylelint src/style/**/*.css",
    "lint-es": "eslint --cache --cache-location '.eslintcache/' --ext .js --ext .jsx src",
    "lint-prettier": "node prettier.js lint",
    "pretest": "yarn lint",
    "prettier": "node prettier.js write",
    "start": "razzle start",
    "start-with-local-converter": "LOCAL_CONVERTER=true razzle start",
    "start-prod": "cross-env NODE_ENV=production node build/server | bunyan",
    "now-start": "cross-env NODE_ENV=production NOW=true node build/server | bunyan",
    "test-ci": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --maxWorkers=4 --ci",
    "test": "cross-env BABEL_ENV=test NODE_ENV=unittest jest",
    "tdd": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --watch",
    "check-articles-and-write": "NODE_ENV=test CHECK_ARTICLE_SCRIPT=true node -r babel-register scripts/checkArticles.js -write",
    "check-articles": "NODE_ENV=test CHECK_ARTICLE_SCRIPT=true node -r babel-register scripts/checkArticles.js"
  },
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.3.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
