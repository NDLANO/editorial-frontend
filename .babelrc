{
  "presets": [
    [
      "razzle",
      {
        "preset-react": {
          "runtime": "automatic"
        }
      }
    ],
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": [
    ["emotion", { "sourceMap": true }],
    "@babel/plugin-proposal-optional-chaining",
    "@loadable/babel-plugin",
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic",
        "importSource": "@emotion/core"
      }
    ]
  ]
}
